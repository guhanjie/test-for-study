<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <session-config>
    <session-timeout>20</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>TestHttpServletRequest</display-name>
    <servlet-name>TestHttpServletRequest</servlet-name>
    <servlet-class>com.helloweenvsfei.servlet.TestHttpServletRequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestHttpServletRequest</servlet-name>
    <url-pattern>/TestHttpServletRequest/*</url-pattern>
  </servlet-mapping>
</web-app>